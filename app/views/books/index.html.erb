<div class="container">
  <div class="row">
    <div class="col-md-3">
      <%= render 'new', book: @book %>
    </div>
    <div class="col-md-8 offset-md-1">
      <% @books.each do |book| %>
        <%=link_to book.title, book_path(book.id) %>
        <%= image_tag book.user.get_profile_image(100,100) %>
      <% end %>
    </div>
  </div>
</div>